{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --detectOpenHandles --forceExit",
    "web-server": "node ./dist/src/server.js",
    "video-processor": "nodemon ./src/queue.ts",
    "video-server": "nodemon ./src/video-server.ts",
    "db": "docker-compose up -d",
    "server": "concurrently \"npm run web-server\" \"npm run video-processor\" ",
    "start": "npm run web-server",
    "stop": "pm2 delete all",
    "restart": "pm2 restart all",
    "logs": "pm2 logs",
    "dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:up-detached": "docker-compose up -d",
    "docker:build-and-up": "docker-compose up --build -d"
  },
